<div>
  <div>

    <h2>Log in</h2>
    <div>
    <%= form_for(User.new, url: login_path) do |f| %>

      <%=f.label :username%>
      <div>
        <spa>@</span>
          <%= f.text_field :username %>
      </div>

      <div>
        <%= f.submit "log in" %>
      </div>
    <% end %>
    </div>
    <%= render partial: 'layouts/flash_errors'%>
  </div>
</div>